        "build:dev": "webpack --mode=development",
        "server": "cd src/server && NODE_ENV=production ts-node main.ts",
        "server:dev": "cd src/server && cross-env NODE_ENV=development nodemon --signal SIGTERM main.ts",
        "storeReservedName": "cd src/server/scripts && NODE_ENV=production ts-node storeReservedName.ts",
