{
    "name": "@wuspy/asteroids",
    "author": "Jacob Jordan",
    "license": "MIT",
    "private": true,
    "engines": {
        "node": ">= 18",
        "npm": ">= 8"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.59.1",
        "@typescript-eslint/parser": "^5.59.1",
        "eslint": "^8.39.0",
        "rimraf": "^5.0.0",
        "tslib": "^2.5.0",
        "typescript": "^5.0.4"
    },
    "scripts": {
        "lint": "npm run lint --workspaces",
        "dev": "npm run dev -w browser",
        "server": "npm run prod -w server",
        "server:dev": "npm run dev --w server",
        "build": "npm run build -w browser",
        "clean:dependencies": "rimraf node_modules",
        "clean:build": "npm run clean --workspaces",
        "clean": "npm run clean:dependencies && npm run clean:build"
    },
    "workspaces": [
        "browser",
        "core",
        "server"
    ]
}